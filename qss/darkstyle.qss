/*------------------------------------------------------------------------------*/
/* QScrollBar 滚动条 */
/* horizontal 水平滚动条 */
QScrollBar:horizontal{
  height: 18px;
  margin: 2px 16px 2px 16px;
  border: 1px solid #1C3F77;
  border-radius: 4px;
  background-color: #0E1827;
}
QScrollBar::handle:horizontal{
  background-color: #1C3F77;
  border: 1px solid #1C3F77;
  border-radius: 4px;
  min-width: 50px;
}
QScrollBar::handle:horizontal:hover,QScrollBar::handle:horizontal:pressed{
background:#2C62BC;
}

QScrollBar::add-line:horizontal{
  margin: 0px 0px 0px 0px;
  border-image: url("./qss/darkstyle/arrow_right.png");
  height: 12px;
  width: 12px;
  subcontrol-position: right;
  subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal:hover{
  border-image: url("./qss/darkstyle/arrow_right_focus.png");
  height: 12px;
  width: 12px;
  subcontrol-position: right;
  subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal:pressed{
  border-image: url("./qss/darkstyle/arrow_right_pressed.png");
  height: 12px;
  width: 12px;
  subcontrol-position: left;
  subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal{
  margin: 0px 3px 0px 3px;
  border-image: url("./qss/darkstyle/arrow_left.png");
  height: 12px;
  width: 12px;
  subcontrol-position: left;
  subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal:hover{
  border-image: url("./qss/darkstyle/arrow_left_focus.png");
  height: 12px;
  width: 12px;
  subcontrol-position: left;
  subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal:pressed{
  border-image: url("./qss/darkstyle/arrow_left_pressed.png");
  height: 12px;
  width: 12px;
  subcontrol-position: left;
  subcontrol-origin: margin;
}
QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal {
  background: none;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
  background: none;
}
/* vertical 垂直滚动条 */
QScrollBar:vertical{
  width: 18px;
  margin: 16px 2px 16px 2px;
  border: 1px solid #1C3F77;
  border-radius: 4px;
  background-color: #0E1827 ;
}
QScrollBar::handle:vertical{
  background-color: #1C3F77;
  border: 1px solid #1C3F77;
  min-height: 50px;
  border-radius: 4px;
}
QScrollBar::handle:vertical:hover,QScrollBar::handle:vertical:pressed{
background:#2C62BC;
}
QScrollBar::add-line:vertical{
  margin: 3px 0px 3px 0px;
  border-image: url("./qss/darkstyle/arrow_down.png");
  height: 12px;
  width: 12px;
  subcontrol-position: bottom;
  subcontrol-origin: margin;
}
QScrollBar::add-line:vertical:hover {
  border-image: url("./qss/darkstyle/arrow_down_focus.png");
  height: 12px;
  width: 12px;
  subcontrol-position: bottom;
  subcontrol-origin: margin;
}
QScrollBar::add-line:vertical:pressed {
  border-image: url("./qss/darkstyle/arrow_down_pressed.png");
  height: 12px;
  width: 12px;
  subcontrol-position: bottom;
  subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical {
  margin: 3px 0px 3px 0px;
  border-image: url("./qss/darkstyle/arrow_up.png");
  height: 12px;
  width: 12px;
  subcontrol-position: top;
  subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical:hover {
  border-image: url("./qss/darkstyle/arrow_up_focus.png");
  height: 12px;
  width: 12px;
  subcontrol-position: top;
  subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical:pressed {
  border-image: url("./qss/darkstyle/arrow_up_pressed.png");
  height: 12px;
  width: 12px;
  subcontrol-position: top;
  subcontrol-origin: margin;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
  background: none;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
  background: none;
}
