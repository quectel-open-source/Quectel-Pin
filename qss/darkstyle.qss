/*------------------------------------------------------------------------------*/
/* QSlider 滑动控件 */ 
/* horizontal 水平滑动控件 */
QSlider::groove:horizontal{
background:#2A4979;
height:8px;
border-radius:4px;
}
QSlider::add-page:horizontal{
background:#808080;
height:8px;
border-radius:4px;
}
QSlider::sub-page:horizontal{
background:#2A4979;
height:8px;
border-radius:4px;
}
QSlider::handle:horizontal{
width:13px;
margin-top:-3px;
margin-bottom:-3px;
border-radius:6px;
background:qradialgradient(spread:pad,cx:0.5,cy:0.5,radius:0.5,fx:0.5,fy:0.5,stop:0.6 #FFFFFF,stop:1 #1C3F77);
}
QSlider::handle:horizontal:hover{
width:13px;
margin-top:-3px;
margin-bottom:-3px;
border-radius:6px;
background:qradialgradient(spread:pad,cx:0.5,cy:0.5,radius:0.5,fx:0.5,fy:0.5,stop:0.6 #FFFFFF,stop:1 #2C62BC);
}
/* vertical 垂直滑动控件 */
QSlider::groove:vertical{
width:8px;
border-radius:4px;
background:#2A4979;
}
QSlider::add-page:vertical{
width:8px;
border-radius:4px;
background:#808080;
}
QSlider::sub-page:vertical{
width:8px;
border-radius:4px;
background:#2A4979;
}
QSlider::handle:vertical{
height:14px;
margin-left:-3px;
margin-right:-3px;
border-radius:6px;
background:qradialgradient(spread:pad,cx:0.5,cy:0.5,radius:0.5,fx:0.5,fy:0.5,stop:0.6 #FFFFFF,stop:1 #1C3F77);
}
QSlider::handle:vertical:hover{
height:14px;
margin-left:-3px;
margin-right:-3px;
border-radius:6px;
background:qradialgradient(spread:pad,cx:0.5,cy:0.5,radius:0.5,fx:0.5,fy:0.5,stop:0.6 #FFFFFF,stop:1 #2C62BC);
}
