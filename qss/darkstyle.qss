image:url(./qss/darkstyle/checkbox_indeterminate.png);
}
QCheckBox::indicator:indeterminate:disabled,QGroupBox::indicator:indeterminate:disabled,QTreeWidget::indicator:indeterminate:disabled,QListWidget::indicator:indeterminate:disabled{
border: none;
image:url(./qss/darkstyle/checkbox_indeterminate_disabled.png);
}
QCheckBox::indicator:indeterminate:hover, QGroupBox::indicator:indeterminate:hover, QTreeWidget::indicator:indeterminate:hover, QListWidget::indicator:indeterminate:hover{
border: none;
image: url("./qss/darkstyle/checkbox_indeterminate_focus.png");
}
QCheckBox::indicator:indeterminate:pressed, QGroupBox::indicator:indeterminate:pressed, QTreeWidget::indicator:indeterminate:pressed, QListWidget::indicator:indeterminate:pressed{
border: none;
image: url("./qss/darkstyle/checkbox_indeterminate_pressed.png");
}
